@model DateOnly?

@(Html.Kendo()
    .DatePickerFor(m => m)
    .Format("{0:dd MMM yyyy}")
    .HtmlAttributes(new { title = Html.ViewContext.ViewData.TemplateInfo.GetFullHtmlFieldName("") }))